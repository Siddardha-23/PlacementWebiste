          <div class="education">
            <h2 style="font-size: 30px"><b>Education:</b></h2>
                <ul>
                    <li>
                        <h3><b>University College Of Engineering Kakinada(A)</b></h3>
                    <h5><%= data.course%></h5>
                    <h5><%= data.branch%></h5>
                    <h5>B.Tech GPA:<%= data.gpa12%></h5>
                    </li>
                    <li>
                        <h3><b><%= data.intercollege%></b></h3>
                    <h5>Inter GPA:<%= data.gpa12%></h5>
                    </li>
                    <li>
                        <h3><b><%= data.school%></b></h3>
                    <h5>Tenth GPA:<%= data.gpa10%></h5>
                    </li>
                </ul> 
          </div>

          <div class="certifi">
            <h2 style="font-size: 30px"><b>Certifications:</b></h2>
                    <% var certifiArr = data.certifications.split(","); %>
                    <ul>
                      <% for (var i = 0; i < certifiArr.length; i++) { %>
                        <li><h5><%= certifiArr[i] %></h5></li>
                      <% } %>
                    </ul>
          </div>

        <div class="skills">
            <h2 style="font-size: 30px"><b>Skills</b></h2>
            <% var skillsArr = data.skills.split(","); %>
            <ul>
              <% for (var i = 0; i < skillsArr.length; i++) { %>
                <li><h5><%= skillsArr[i] %></h5></li>
              <% } %>
            </ul>
          </div>

        <div>
            <h2 style="font-size: 30px"><b>Achievements:</b></h2>
            <% var achiArr = data.achievements.split(","); %>
            <ul>
              <% for (var i = 0; i < achiArr.length; i++) { %>
                <li><h5><%= achiArr[i] %></h5></li>
              <% } %>
            </ul>
        </div><br>

          <div>
            <h2 style="font-size: 30px"><b>Projects:</b></h2>
            <% var proArr = data.projects.split(","); %>
            <ul>
              <% for (var i = 0; i < proArr.length; i++) { %>
                <li><h5><%= proArr[i] %></h5></li>
              <% } %>
            </ul>
        </div><br>

    </body>
